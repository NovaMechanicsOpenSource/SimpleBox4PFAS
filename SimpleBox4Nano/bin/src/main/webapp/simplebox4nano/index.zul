<?page id="thePage" title="SimpleBox4Nano" contentType="text/html;charset=UTF-8"?>
<zk xmlns:native="native" xmlns:client="client">

	<style dynamic="true">
		.z-row-content, .z-row-inner, .z-rows, tr.z-row, .z-grid-odd,
		.z-grid-body, .z-grid-header, .z-grid-header-border, .z-cell,
		.z-grid, .z-listbox, .z-listbox-body,
		.z-listbox-odd,.z-listbox-header, .z-listbox-header-border {
		background: transparent !important; border: none !important;
		padding:5px !important; grid-row-gap: 5px !important;

		tr.z-loading { top: 50% !important; left: 45% !important; align:
		center; vertical-align: middle; }

		a:link{ color:blue; }

		a:visited { color:green; }

		a:hover { color: green; }

		a:active { color:red; }

		a:selected{ color:red; }
	</style>

	<window id="mainWin"
		apply="eu.nanosolveit.simplebox4nano.SimpleBox4NanoModelController"
		border="normal" hflex="1">

		<hbox id="titleSimple" align="center">
			<html>
				<div align="center" width="100%" visible="true">
					<table>
						<tr>
							<td>
								<img border="0"
									src="../resources/SmallLogo.jpg" width="140" height="80" />
							</td>
							<td>
								<h1 style="font-size:30px;">
									SimpleBox4Nano
								</h1>
							</td>
						</tr>
					</table>
					<br />
				</div>
			</html>
		</hbox>

		<a label="User Guide" href="instructions.zul" />
		<separator height="20px" />

		<vlayout>
			<label value="Landscape scenarios" style="font-size:1.5em;" />
			<separator />
			<hlayout style="border: 1px groove black" />
		</vlayout>

		<grid align="left">
			<columns>
				<column label="" width="420px" />
				<column label="" width="140px" />
				<column label="" width="140px" />
				<column label="" width="140px" />
			</columns>
			<rows>
				<row>
					<combobox id="scenarios" readonly="true"
						width="300px" />

					<button id="adjust" label="Adjust ..." />
					<button id="save" label="Save ..." />
					<button id="load" label="Load ..." upload="true" />
				</row>

			</rows>
		</grid>

		<separator height="20px" />

		<vlayout>
			<label value="Nanomaterials properties"
				style="font-size:1.5em;" />
			<separator />
			<hlayout style="border: 1px groove black;" />
		</vlayout>

		<grid align="left">
			<columns>
				<column label="" width="420px" />
				<column label="" />
				<column label="" />
			</columns>
			<rows>
				<row tooltip="msg">
					Nanomaterial
					<combobox id="nanomaterials" readonly="true"
						width="145px" />

					<popup id="msg" width="300px">
						Choose a nanomaterial in this list to prefill
						properties with properties for this
						nanomaterial.
					</popup>
				</row>
				<row tooltip="msg1">
					<label value="Molecular weight [gr/mol]"
						tooltip="sgfdgfdgdfgfd" />
					<doublebox id="molweight"
						constraint="no negative,no empty" />

					<popup id="msg1" width="300px">
						The molecular weight of the nanomaterial.
					</popup>
				</row>
				<row tooltip="msg2">
					<label value="Primary radius [nm]" />
					<doublebox id="radius"
						constraint="no negative,no empty" />

					<popup id="msg2" width="300px">
						Primary radius of the nanomaterial.
					</popup>
				</row>
				<row tooltip="msg3">
					<label value="Primary density [kg/m3]" />
					<doublebox id="density"
						constraint="no negative,no empty" />

					<popup id="msg3" width="300px">
						Density of the nanomaterial.
					</popup>
				</row>
				<row tooltip="msg4">
					<label
						value="Hamaker constant heteroagglomerate (ENP, water, SiO2)" />
					<doublebox id="hamaker"
						constraint="no negative,no empty" />

					<popup id="msg4" width="300px">
						Hamaker constant between the engineered
						nanoparticle (ENP) and soil grains.
					</popup>
				</row>
			</rows>
		</grid>

		<separator height="20px" />

		<vlayout>
			<label
				value="Nanomaterial-Environment interaction parameters "
				style="font-size:1.5em;" />
			<separator />
			<hlayout style="border: 1px groove black;" />
			<separator />
		</vlayout>


		<!-- START Attachment efficiency -->
		<label value="Attachement efficiency" style="font-size:1.3em;" />

		<separator bar="true" height="20px" />

		<listbox id="attachmentEfficiency">

			<listhead sizable="false">
				<listheader label="" width="150px" />
				<listheader label="" />
			</listhead>

			<listitem id="freshWaterItem">
				<listcell>
					<radiogroup id="freshWaterRadio">
						<vlayout>
							<radio label="coarse" selected="true" />
							<radio label="detailed" />
						</vlayout>
					</radiogroup>
				</listcell>
				<listcell tooltip="msg5">
					<hbox id="freshWaterCoarse" visible="true"
						align="center">
						<label value="In fresh water [-]" />
						<separator width="160px" />
						<doublebox id="freshWater"
							constraint="no negative,no empty" />
					</hbox>
					<hbox id="freshWaterDetailed" visible="false">
						<grid>
							<columns>
								<column label="" width="400px" />
								<column label="" />
							</columns>
							<rows>
								<row tooltip="msg5a">
									<label
										value="Lake water NCs (less than 450 nm) [-]" width="200px" />
									<doublebox id="lakeWaterNC"
										constraint="no negative,no empty" />

									<popup id="msg5a" width="300px">
										Attachment efficiency between
										nanoparticles and natural
										colloids (environmental
										particles smaller than 450 nm in
										diameter) in lake water.
									</popup>
								</row>
								<row tooltip="msg5b">
									<label
										value="Lake water SPM (greater than 450 nm) [-]" />
									<doublebox id="lakeWaterSPM"
										constraint="no negative,no empty" />

									<popup id="msg5b" width="300px">
										Attachment efficiency between
										nanoparticles and suspended
										particulate matter
										(environmental particles greater
										than 450 nm in diameter) in lake
										water.
									</popup>
								</row>

								<row tooltip="msg5c">
									<label
										value="Fresh water NCs (less than 450 nm) [-]" />
									<doublebox id="freshWaterNC"
										constraint="no negative,no empty" />

									<popup id="msg5c" width="300px">
										Attachment efficiency between
										nanoparticles and natural
										colloids (environmental
										particles smaller than 450 nm in
										diameter) in fresh water, e.g.
										rivers.
									</popup>

								</row>

								<row tooltip="msg5d">
									<label
										value="Fresh water SPM (greater than 450 nm) [-]" />
									<doublebox id="freshWaterSPM"
										constraint="no negative,no empty" />

									<popup id="msg5d" width="300px">
										Attachment efficiency between
										nanoparticles and suspended
										particulate matter
										(environmental particles greater
										than 450 nm in diameter) in
										fresh water, e.g. rivers.
									</popup>

								</row>
							</rows>
						</grid>
					</hbox>
					<hbox id="freshWaterDetailedReporting"
						visible="false">
						<label id="freshWaterCompact"
							value="In fresh water" />
					</hbox>

					<popup id="msg5" width="300px">
						Attachment efficiency between a nanoparticle and
						environmental particle in fresh water.
						Attachment efficiency is the probability of a
						collision between a nanoparticle and an
						environmental particle resulting in sticking
						(aggregation).
					</popup>

				</listcell>
			</listitem>

			<listitem id="attachmentSoilDbItem">
				<listcell>
					<radiogroup id="attachmentSoilDbRadio">
						<vlayout>
							<radio label="coarse" selected="true" />
							<radio label="detailed" />
						</vlayout>
					</radiogroup>
				</listcell>
				<listcell tooltip="msg6">
